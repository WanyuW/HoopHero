<?xml version="1.0" ?>

<world name="demo_world" gravity="0.0 0.0 -9.81">

	<robot name="panda">
		<model dir="./resources" path="panda_arm.urdf" name="panda" />
		<origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
	</robot>

	<static_object name="Base">
		<origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
	    <visual>
	        <origin xyz="0.0 0.0 -0.00635" rpy="0 0 0" />
	        <geometry>
				<box size="5 5 0.0127" />
	        </geometry>
	        <material name="material">
		        <color rgba="0.1 0.1 0.1 1.0" />
	        </material>
	    </visual>
		<collision>
	        <origin xyz="0.0 0.0 -0.00635" rpy="0 0 0" />
	        <geometry>
	            <box size="5 5 0.0127" />
	        </geometry>
	    </collision>
	</static_object>

	<dynamic_object name="cup">
		<origin xyz="1.0 0.0 1.0" rpy="0 0 0" />
		<inertial>
        	<origin xyz="0 0 0" rpy="0 0 0" />
        	<mass value="10" />
        	<inertia ixx="1" iyy="1" izz="1" ixy="0" ixz="0" iyz="0" />
      	</inertial>

	    <visual>
	        <origin xyz="0 0 0" rpy="0 0 0" />
			<geometry>
	            <mesh filename="../../model/test_objects/meshes/visual/cup.obj" />
	        </geometry>
	    </visual>

		<visual>
	        <origin xyz="0 0 0.06" rpy="0 0 0" />
	        <geometry>
	            <box size="0.1 0.1 0.15" />
	        </geometry>
	        <material name="material">
		        <color rgba="1 1 1 1.0" />
	        </material>
	    </visual>

	    <collision>
	        <origin xyz="0 0 0.06" rpy="0 0 0" />
	        <geometry>
	            <box size="0.1 0.1 0.15" />
	        </geometry>
	    </collision>
	</dynamic_object>

	<light name="light1" type="directional">
		<position xyz="2.0 -2.0 2.0" />
		<lookat xyz="0.0 0.0 0.0" />
	</light>

	<light name="light2" type="directional">
		<position xyz="2.0 2.0 2.0" />
		<lookat xyz="0.0 0.0 0.0" />
	</light>

	<camera name="camera_fixed">
		<position xyz="0 -2 2.5" />
		<vertical xyz="0.0 0.0 1.0" />
		<lookat xyz="0 0 0.5" />
	</camera>

</world>
